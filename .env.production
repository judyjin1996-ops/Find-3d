# 生产环境配置
NODE_ENV=production
VITE_APP_TITLE=Find 3D - 三维素材搜索平台
VITE_APP_VERSION=2.0.0
VITE_APP_DESCRIPTION=专为三维设计师打造的智能素材搜索平台

# API配置
VITE_API_BASE_URL=https://api.find3d.com
VITE_API_TIMEOUT=30000

# 爬虫配置
VITE_CRAWLER_ENABLED=true
VITE_CRAWLER_MAX_CONCURRENT=3
VITE_CRAWLER_REQUEST_DELAY=2000
VITE_CRAWLER_TIMEOUT=30000

# 缓存配置
VITE_CACHE_ENABLED=true
VITE_CACHE_EXPIRY=300000
VITE_CACHE_MAX_SIZE=50

# 监控配置
VITE_ANALYTICS_ENABLED=true
VITE_ERROR_REPORTING_ENABLED=true

# 功能开关
VITE_FEATURE_ADVANCED_SEARCH=true
VITE_FEATURE_CUSTOM_WEBSITES=true
VITE_FEATURE_EXPORT_IMPORT=true
VITE_FEATURE_REAL_TIME_SEARCH=true

# 安全配置
VITE_SECURITY_CSP_ENABLED=true
VITE_SECURITY_HTTPS_ONLY=true

# 性能配置
VITE_PERFORMANCE_LAZY_LOADING=true
VITE_PERFORMANCE_IMAGE_OPTIMIZATION=true
VITE_PERFORMANCE_BUNDLE_ANALYSIS=false