<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Find 3D - 三维素材搜索平台</title>
    <meta name="description" content="专为三维设计师打造的素材搜索平台，支持多网站搜索聚合" />
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        body {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            min-height: 100vh;
            padding: 20px;
        }
        
        .container {
            max-width: 1200px;
            margin: 0 auto;
        }
        
        .header {
            text-align: center;
            margin-bottom: 40px;
        }
        
        .header h1 {
            font-size: 3rem;
            color: white;
            margin-bottom: 10px;
            text-shadow: 2px 2px 4px rgba(0,0,0,0.3);
        }
        
        .header p {
            font-size: 1.2rem;
            color: rgba(255,255,255,0.9);
            text-shadow: 1px 1px 2px rgba(0,0,0,0.3);
        }
        
        .search-box {
            background: white;
            border-radius: 15px;
            padding: 30px;
            box-shadow: 0 10px 30px rgba(0,0,0,0.2);
            margin-bottom: 30px;
        }
        
        .search-form {
            display: flex;
            gap: 15px;
            align-items: center;
        }
        
        .search-input {
            flex: 1;
            padding: 15px 20px;
            font-size: 1.1rem;
            border: 2px solid #e1e5e9;
            border-radius: 10px;
            outline: none;
            transition: border-color 0.3s;
        }
        
        .search-input:focus {
            border-color: #667eea;
        }
        
        .search-btn {
            padding: 15px 30px;
            font-size: 1.1rem;
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            color: white;
            border: none;
            border-radius: 10px;
            cursor: pointer;
            transition: all 0.3s;
        }
        
        .search-btn:hover {
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(0,0,0,0.2);
        }
        
        .features {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 20px;
            margin-bottom: 30px;
        }
        
        .feature-card {
            background: white;
            border-radius: 15px;
            padding: 25px;
            text-align: center;
            box-shadow: 0 5px 15px rgba(0,0,0,0.1);
            transition: transform 0.3s;
        }
        
        .feature-card:hover {
            transform: translateY(-5px);
        }
        
        .feature-icon {
            font-size: 3rem;
            margin-bottom: 15px;
        }
        
        .feature-title {
            font-size: 1.3rem;
            margin-bottom: 10px;
            color: #333;
        }
        
        .feature-desc {
            color: #666;
            line-height: 1.5;
        }
        
        .results {
            background: white;
            border-radius: 15px;
            padding: 30px;
            box-shadow: 0 10px 30px rgba(0,0,0,0.2);
            display: none;
        }
        
        .results.show {
            display: block;
        }
        
        .results-grid {
            display: grid;
            grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));
            gap: 20px;
            margin-top: 20px;
        }
        
        .result-card {
            border: 1px solid #e1e5e9;
            border-radius: 10px;
            overflow: hidden;
            transition: transform 0.3s, box-shadow 0.3s;
            cursor: pointer;
        }
        
        .result-card:hover {
            transform: translateY(-3px);
            box-shadow: 0 5px 15px rgba(0,0,0,0.1);
        }
        
        .result-image {
            width: 100%;
            height: 200px;
            object-fit: cover;
            background: #f0f0f0;
            display: flex;
            align-items: center;
            justify-content: center;
            color: #999;
            font-size: 14px;
        }
        
        .result-content {
            padding: 15px;
        }
        
        .result-title {
            margin-bottom: 10px;
            color: #333;
            font-size: 1.1rem;
        }
        
        .result-meta {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }
        
        .result-source {
            color: #666;
        }
        
        .result-price {
            font-weight: bold;
        }
        
        .result-price.free {
            color: #28a745;
        }
        
        .result-price.paid {
            color: #007bff;
        }
        
        .footer {
            text-align: center;
            margin-top: 40px;
        }
        
        .footer p {
            color: rgba(255,255,255,0.8);
            text-shadow: 1px 1px 2px rgba(0,0,0,0.3);
        }
        
        .loading {
            text-align: center;
            padding: 20px;
            color: #666;
        }
        
        .status-info {
            background: #f8f9fa;
            border-radius: 10px;
            padding: 15px;
            margin-bottom: 20px;
            border-left: 4px solid #667eea;
        }
        
        .github-link {
            display: inline-block;
            margin-top: 20px;
            padding: 10px 20px;
            background: rgba(255,255,255,0.2);
            color: white;
            text-decoration: none;
            border-radius: 8px;
            transition: all 0.3s;
        }
        
        .github-link:hover {
            background: rgba(255,255,255,0.3);
            transform: translateY(-2px);
        }
        
        @media (max-width: 768px) {
            .header h1 {
                font-size: 2rem;
            }
            
            .search-form {
                flex-direction: column;
            }
            
            .search-input {
                width: 100%;
            }
            
            .search-btn {
                width: 100%;
            }
        }
    </style>
</head>
<body>
    <div class="container">
        <!-- 头部 -->
        <div class="header">
            <h1>🎨 Find 3D</h1>
            <p>专为三维设计师打造的素材搜索平台</p>
            <a href="https://github.com/judyjin1996-ops/Find-3d" class="github-link" target="_blank">
                📂 查看GitHub项目
            </a>
        </div>

        <!-- 搜索框 -->
        <div class="search-box">
            <div class="search-form">
                <input type="text" class="search-input" id="searchInput" placeholder="搜索3D素材..." value="日本建筑">
                <button class="search-btn" onclick="performSearch()">🔍 搜索</button>
            </div>
        </div>

        <!-- 功能特色 -->
        <div class="features">
            <div class="feature-card">
                <div class="feature-icon">🔍</div>
                <h3 class="feature-title">多网站搜索</h3>
                <p class="feature-desc">同时搜索魔顿网、CG资源网、书生CG资源站等多个平台</p>
            </div>
            <div class="feature-card">
                <div class="feature-icon">🎯</div>
                <h3 class="feature-title">统一结果展示</h3>
                <p class="feature-desc">标准化显示预览图、价格、免费状态等关键信息</p>
            </div>
            <div class="feature-card">
                <div class="feature-icon">⚙️</div>
                <h3 class="feature-title">灵活配置</h3>
                <p class="feature-desc">自定义搜索源和显示字段，个性化你的搜索体验</p>
            </div>
        </div>

        <!-- 搜索结果 -->
        <div class="results" id="results">
            <div class="status-info">
                <h3>搜索状态</h3>
                <p id="searchStatus">点击搜索按钮开始搜索...</p>
            </div>
            
            <h2>搜索结果</h2>
            <div class="loading" id="loading" style="display: none;">
                正在搜索中，请稍候...
            </div>
            <div class="results-grid" id="resultsGrid"></div>
        </div>

        <!-- 底部信息 -->
        <div class="footer">
            <p>简约设计 · 高效搜索 · 专业工具</p>
            <p style="margin-top: 10px; font-size: 0.9rem;">
                <a href="https://github.com/judyjin1996-ops/Find-3d" style="color: rgba(255,255,255,0.8);">GitHub项目</a> | 
                <a href="https://judyjin1996-ops.github.io/Find-3d/" style="color: rgba(255,255,255,0.8);">在线演示</a>
            </p>
        </div>
    </div>

    <script>
        // 搜索功能
        function performSearch() {
            const query = document.getElementById('searchInput').value.trim();
            if (!query) return;

            const results = document.getElementById('results');
            const loading = document.getElementById('loading');
            const resultsGrid = document.getElementById('resultsGrid');
            const searchStatus = document.getElementById('searchStatus');

            // 显示结果区域和加载状态
            results.classList.add('show');
            loading.style.display = 'block';
            resultsGrid.innerHTML = '';
            searchStatus.textContent = `正在搜索"${query}"...`;

            // 模拟搜索延迟
            setTimeout(() => {
                loading.style.display = 'none';
                searchStatus.innerHTML = `
                    <strong>搜索完成！</strong><br>
                    魔顿网: 找到 4 个结果 ✅<br>
                    CG资源网: 找到 3 个结果 ✅<br>
                    书生CG资源站: 找到 5 个结果 ✅
                `;
                
                // 模拟搜索结果（使用可靠的图片源）
                const mockResults = [
                    {
                        id: 1,
                        title: `高质量${query}模型`,
                        image: 'data:image/svg+xml;base64,',
                        source: '魔顿网',
                        price: 99,
                        isFree: false,
                        url: 'http://www.modown.cn'
                    },
                    {
                        id: 2,
                        title: `免费${query}素材`,
                        image: 'data:image/svg+xml;base64,',
                        source: 'CG资源网',
                        price: 0,
                        isFree: true,
                        url: 'https://www.cgown.com'
                    },
                    {
                        id: 3,
                        title: `精品${query}模型包`,
                        image: 'data:image/svg+xml;base64,',
                        source: '书生CG资源站',
                        price: 199,
                        isFree: false,
                        url: 'https://c4dsky.com'
                    },
                    {
                        id: 4,
                        title: `${query}材质贴图`,
                        image: 'data:image/svg+xml;base64,',
                        source: '魔顿网',
                        price: 0,
                        isFree: true,
                        url: 'http://www.modown.cn'
                    },
                    {
                        id: 5,
                        title: `${query}场景模型`,
                        image: 'data:image/svg+xml;base64,',
                        source: 'CG资源网',
                        price: 149,
                        isFree: false,
                        url: 'https://www.cgown.com'
                    },
                    {
                        id: 6,
                        title: `${query}动画模型`,
                        image: 'data:image/svg+xml;base64,',
                        source: '书生CG资源站',
                        price: 0,
                        isFree: true,
                        url: 'https://c4dsky.com'
                    }
                ];

                // 渲染结果
                resultsGrid.innerHTML = mockResults.map(result => `
                    <div class="result-card" onclick="openResult('${result.url}', '${result.title}')">
                        <div class="result-image">
                            <img src="${result.image}" alt="${result.title}" style="width: 100%; height: 100%; object-fit: cover;" 
                                 onerror="this.parentElement.innerHTML='<div style=\\'display:flex;align-items:center;justify-content:center;height:100%;color:#999;\\'>🖼️ 预览图</div>'">
                        </div>
                        <div class="result-content">
                            <h3 class="result-title">${result.title}</h3>
                            <div class="result-meta">
                                <span class="result-source">${result.source}</span>
                                <span class="result-price ${result.isFree ? 'free' : 'paid'}">
                                    ${result.isFree ? '免费' : `¥${result.price}`}
                                </span>
                            </div>
                        </div>
                    </div>
                `).join('');
            }, 1500);
        }

        // 打开结果链接
        function openResult(url, title) {
            if (confirm(`确定要访问 "${title}" 的原始页面吗？\n\n将在新标签页打开: ${url}`)) {
                window.open(url, '_blank');
            }
        }

        // 回车搜索
        document.getElementById('searchInput').addEventListener('keypress', function(e) {
            if (e.key === 'Enter') {
                performSearch();
            }
        });

        // 页面加载完成后自动搜索
        window.addEventListener('load', function() {
            console.log('🎨 Find 3D 已加载完成！');
            console.log('GitHub Pages版本 - 修复空白页问题');
            
            // 自动执行一次搜索展示效果
            setTimeout(() => {
                performSearch();
            }, 1000);
        });
    </script>
</body>
</html>
